{
  "breadcrumbs.enabled": false,
  "editor.fontSize": 13,
  "editor.fontFamily": "Fira Code",
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "trailing",
  "editor.wrappingIndent": "none",
  "editor.lineHeight": 16,
  "editor.tabCompletion": "on",
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": false,
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.rulers": [
      125
  ],
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "explorer.openEditors.visible": 0,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
      ".idea": true
  },
  "json.maxItemsComputed": 50000,
  "editor.maxTokenizationLineLength": 50000,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.23,
  "terminal.integrated.cursorStyle": "block",
  "window.zoomLevel": 0,
  "workbench.tree.indent": 12,
  "workbench.colorTheme": "Monokai Dimmed",
  "workbench.iconTheme": "file-icons",
  "workbench.activityBar.visible": false,
  "workbench.editor.showIcons": true,
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#6d839e",
    "terminalCursor.foreground": "#6d839e",
    "editorCursor.background": "#1d1d1d",
    "statusBar.background" : "#1d1d1d"
  },
  // leave me alone
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeLens": false,
  "extensions.ignoreRecommendations": false,
  "explorer.autoReveal": false,
  "keyboard.touchbar.enabled": false,
  "update.showReleaseNotes": false,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "workbench.enableExperiments": false,
  "workbench.tips.enabled": false
}
