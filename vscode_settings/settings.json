{
  "editor.fontSize": 13,
  "editor.fontFamily": "Fira Code",
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "all",
  "editor.wrappingIndent": "none",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "editor.lineHeight": 20,
  "editor.tabCompletion": "on",
  "editor.rulers": [
      125
  ],
  "editor.minimap.enabled": false,
  "workbench.startupEditor": "none",
  "editor.renderLineHighlight": "gutter",
  "workbench.colorCustomizations":{
    "activityBar.background": "#282c34",
    "activityBar.border": "#1d2021",
    "activityBar.foreground": "#7f848e",
    "editorGroup.emptyBackground": "#32302f",
    "editorCursor.foreground": "#7f848e",
    "tab.activeBackground": "#282c34",
    "tab.activeBorder": "#1d2021",
    "tab.activeForeground": "#7f848e",
    "sideBar.background": "#282c34",
    "sideBar.border": "#1d2021",
    "sideBar.foreground": "#7f848e",
    "statusBar.foreground": "#7f848e"
},
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "One Monokai",
  "workbench.iconTheme": "file-icons-colourless",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.23,
  "editor.minimap.renderCharacters": false,
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": false,
  "workbench.activityBar.visible": false,
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.showIcons": false,
  "explorer.openEditors.visible": 0,
  "gitlens.currentLine.enabled": false,
  "files.autoSave": "onFocusChange"
}
