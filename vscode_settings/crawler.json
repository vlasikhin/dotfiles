{
	"crawler": {
		"prefix": "crw",
		"body": [
			"class CHANGE_ME < ApplicationCrawler || IntlCrawler",
			"  domain \"\"",
			"  start_urls \"\"", 
			"  seller company: \"\",",
			"         address: \"\",", 
			"         phone:   \"\",",
			"         website: \"\"",
			"",
			"  def parse",
			"    byebug",
			"    xpath(\"\").each do |a|",
			"      yield request(url: a[:href], callback: :parse_category, data: data)",
			"    end",
			"  end",
			"",
			"  def parse_category",
			"    xpath(\"\").each do |a|",
			"      yield request(url: a[:href], callback: :parse_listing, data: data)",
			"    end",
			"  end",
			"",
			"  def parse_listing",
			"    field :id,           value: MurmurHash3::V32.str_hash(url.to_s).to_i",
			"    field :url,          value: url.to_s",
			"    field :domain,       value: domain",
			"    field :condition,    value: \"used\"",
			"    field :location,     value: \"\"",
			"    field :price,        value: 0",
			"    field :categories,   value:  categories.to_a",
			"    field :title,        value: fields.values_at(:year, :manufacturer, :model).compact.join(\" \")",
			"    # yield fields",
			"  end",
			"end"
		],
		"description": "crawler"
	},

	"require_relative": {
		"prefix": "req",
		"body": [
			"require_relative \"\"",
			"class CHANGE_ME",
			"  domain \"\", lang: ",
			"  translate from:",
			"end"
		],
		"description": "require_relative"
	}
}