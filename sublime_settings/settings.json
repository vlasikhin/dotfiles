{
  "block_caret": true,
  "ensure_newline_at_eof_on_save": true,
  "font_face": "Fira Code",
  "draw_indent_guides": false,
  "font_options":
  [
    "no_liga",
    "no_clig",
    "no_calt"
  ],
  "font_size": 13,
  "highlight_line": true,
  "ignored_packages":
  [
    "Vintage",
  ],
  "rulers":
  [
    125
  ],
  "save_on_focus_lost": true,
  "show_encoding": true,
  "show_line_annotation": false,
  "show_status_bar_text": true,
  "tab_size": 2,
  "translate_tabs_to_spaces": true,
  "trim_trailing_white_space_on_save": true,
  "word_wrap": false,
  "theme": "auto",
  "color_scheme": "Packages/Alabaster Color Scheme/Alabaster.sublime-color-scheme",
}
